#include <iostream>
using namespace std;
enum enOddOrEven { Odd = 1, Even };
int ReadN()
{
	int N;
	cout << "Please Enter Number?" << endl;
	cin >> N;
	return N;
}
enOddOrEven CheckOddOrEven(int Number)
{
	if (Number % 2 != 0)
		return enOddOrEven::Odd;
	else
		return enOddOrEven::Even;

}
int PrintSumOddNumberFrom1_To_N(int N)
{
	int SumOdd = 0;
	for (int i = 1; i <= N; i++)
	{
		if (CheckOddOrEven(i) == enOddOrEven::Odd)
		{
			SumOdd += i;
		}
	}
	return SumOdd;
}      
int main()
{
	int N = ReadN();
	cout <<"Your SumOdd: " << PrintSumOddNumberFrom1_To_N(N) << endl;

	return 0;



}


